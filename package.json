    "@remix-run/react": "^2.16.2",
    "@tanstack/react-query": "^5.71.0",
    "@types/google.maps": "^3.58.1",
    "axios": "^1.8.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "prisma": "^6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "swiper": "^11.2.6",
    "zod": "^3.24.2"
